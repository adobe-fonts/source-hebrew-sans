begin GlyphSet UPPERCASE
	#Latin
		^[A-Z][A-Z]?(macron|breve|circumflex|dieresis|horn|dotbelow)?(grave|acute|circumflex|tilde|dieresis|ring|caron|slash|cedilla|horn|breve|macron|ogonek|croat|dotaccent|commaaccent|hungarumlaut|dot|bar|th|line|hook|stroke)?(above|below|dotaccent)?$
		^(Germandbls|Schwa)$
	#Greek
		^(Alpha|Beta|Gamma|Delta|Epsilon|Zeta|Eta|Theta|Iota|Kappa|Lambda|Mu|Nu|Xi|Omicron|Pi|Rho|Sigma|Tau|Upsilon|Phi|Chi|Psi|Omega)(iotasub)?(tonos|grave|acute|dieresis|lenis|asper|breve|macron|.greek)?(grave|acute|tilde)?(.alt)?$
	#Cyrillic
		^(A|E|I|O|U|Schwa)(grave|breve|macron)?(cyr|short|ukran)$
		^(Be|Ve|Ghe|De|Ie|Zhe|Ze|Ka|El|Em|En|Pe|Er|Es|Te|Ef|Ha|Tse|Che|Sha|Shcha|Hard|Yeru|Soft|Yu|Ya|Io|Dje|Gje|Dze|Yi|Je|Lje|Nje|Tshe|Kje|Dzhe|Yat|Fita|Izhitsa)(grave|ukran|up)?$
	#Punctuation
		^(exclam|question)$
		^(trademark|registered|servicemark)$
		^(ampersand|section|paragraph)$
	#Math
		^(Delta|Omega).math$
end GlyphSet UPPERCASE


begin GlyphSet LOWERCASE
	#Latin
		^(dotless|germandbl|schw)?[a-z](macron|breve|circumflex|dieresis|horn|dotbelow)?(grave|acute|circumflex|tilde|dieresis|ring|caron|slash|cedilla|horn|breve|macron|ogonek|croat|dotaccent|commaaccent|hungarumlaut|dot|bar|th|line|hook|stroke|tail|ng|script|greenlandic|apostrophe)?(above|below|dotaccent)?(\.)?(alt|d|trk)?$
		^[afo][eil]$
		^(f)_(f|t)$
	#Greek
		^(alpha|beta|gamma|delta|epsilon|zeta|eta|theta|iota|kappa|lambda|mu|nu|xi|omicron|pi|rho|sigma|tau|upsilon|phi|chi|psi|omega)(iotasub)?(.alt|.end|.greek|tonos|dieresis|lenis|asper|acute|grave|breve|macron|tilde)?(tonos|acute|grave|tilde)?$
		^(stigma|koppa|sampi|digamma)$
	#Cyrillic
		^(a|e|i|o|u|schwa)(grave|breve|macron)?(cyr|short|ukran)$
		^(be|ve|ghe|de|ie|zhe|ze|ka|el|em|en|pe|er|es|te|ef|ha|tse|che|sha|shcha|hard|yeru|soft|yu|ya|io|dje|gje|dze|yi|je|lje|nje|tshe|kje|dzhe|yat|fita|izhitsa|palochka)(ukran|up|ital)?$
	#Punctuation
		^(exclam|question)down$
		^(dagger|daggerdbl)$
end GlyphSet LOWERCASE


